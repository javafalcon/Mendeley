isbn = {1532-4435},
journal = {Journal of Machine Learning Research},
keywords = {convex optimization,kernel methods,learning kernels,model selection,semidefinite programming,support vector machines,transduction},
language = {English},
pages = {27--72},
title = {{Learning the kernel matrix with semidefinite programming}},
volume = {5},
