isbn = {0196-9781},
journal = {Peptides},
keywords = {Anti-cancer,Anti-viral,Frog skin,Host-defense peptide,Immunomodulatory,Type 2 diabetes},
number = {0},
pages = {67--77},
title = {{Potential therapeutic applications of multifunctional host-defense peptides from frog skin as anti-cancer, anti-viral, immunomodulatory, and anti-diabetic agents}},
