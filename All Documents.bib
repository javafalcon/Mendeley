journal = {Journal of proteome research},
keywords = {25{\%} sequence identity cutoff,algorithm,amino-acid-composition,amphiphilic pseudo amino acid composition,cellular networking,classification,fusion classifier,gene ontology,localization,oet-knn,organelle,representation,sequences,sorting signals,structural classes,support vector machines},
